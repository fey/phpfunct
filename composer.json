{
    "name": "funct/funct",
    "description": "A PHP library with commonly used code blocks",
    "authors": [
        {
            "name": "Aurimas Niekis",
            "email": "aurimas@niekis.lt"
        }
    ],
    "type": "library",
    "license": "MIT",
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "squizlabs/php_codesniffer": "4.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Funct\\": "src"
        },
        "files": [
            "src/Collection.php",
            "src/General.php",
            "src/Invoke.php",
            "src/Object.php",
            "src/Strings.php"
        ]
    },
    "scripts": {
        "lint": "phpcs src",
        "lint-fix": "phpcbf src",
        "test": "vendor/bin/phpunit",
        "test-ci": "vendor/bin/phpunit -c phpunit.xml.ci"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
